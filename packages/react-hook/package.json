    "@types/react-dom": "^18.3.1",
    "glob": "^11.0.0",
    "react": "^18.3.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "react": "^18 || ^19"
