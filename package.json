{
  "name": "starkit",
  "private": true,
  "namespace": "@starkit",
  "description": "A universal packages for starkit",
  "main": "index.js",
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "start": "turbo run start"
  },
  "keywords": [
    "starkit",
    "utils",
    "lifecycle",
    "disposable",
    "event"
  ],
  "author": "tangye1234@gmail.com",
  "license": "MIT",
  "packageManager": "pnpm@9.0.0+",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@types/node": "^18.18.0",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "turbo": "^2.1.3",
    "typescript": "^5.6.3"
  },
  "pnpm": {
    "overrides": {
      "eslint": "9.12.0",
      "glob": "11.0.0",
      "tsup": "8.3.0",
      "typescript": "5.6.3"
    }
  }
}
